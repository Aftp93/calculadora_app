    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        num = Double.parseDouble(jText_Resultado.getText());
        calculation = 1;
        jText_Resultado.setText(" ");
        jLabel2.setText(num + "+");
    }                                        

